CREATE PROCEDURE `CompetentieInsert`
(
	OUT pId INT ,
	IN pName NVARCHAR (200) ,
)
BEGIN
INSERT INTO `Competentie`
	(
		`Competentie`.`Naam`,
	)
	VALUES
	(
		pName,
  );
	SELECT LAST_INSERT_ID() INTO pId;
END //
DELIMITER ;

